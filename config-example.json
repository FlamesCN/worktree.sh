{
  "language": "en",
  "workingRepo": {
    "path": "/Users/you/Developer/your-project",
    "branch": "main"
  },
  "worktreeAdd": {
    "copyEnv": {
      "enabled": true,
      "files": [
        ".env",
        ".env.local"
      ]
    },
    "installDeps": {
      "enabled": true,
      "command": "npm ci"
    },
    "serveDev": {
      "enabled": true,
      "command": "npm run dev",
      "loggingPath": "tmp"
    }
  }
}
