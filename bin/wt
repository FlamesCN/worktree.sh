#!/usr/bin/env bash
set -euo pipefail

readonly SCRIPT_SOURCE="${BASH_SOURCE[0]:-${0}}"
script_dir_tmp=""
if script_dir_tmp=$(cd "$(dirname "$SCRIPT_SOURCE")" 2> /dev/null && pwd); then
  :
else
  script_dir_tmp=$(pwd)
fi
readonly SCRIPT_DIR="$script_dir_tmp"

# shellcheck source=lib/runtime.sh disable=SC1091
. "$SCRIPT_DIR/lib/runtime.sh"
# shellcheck source=lib/commands.sh disable=SC1091
. "$SCRIPT_DIR/lib/commands.sh"

wt_main "$@"
